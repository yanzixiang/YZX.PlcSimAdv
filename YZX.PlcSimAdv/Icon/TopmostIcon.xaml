<UserControl x:Class="PlcSim.Compact.TopmostIcon"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             Padding="2">
  <Grid x:Name="LayoutRoot" Width="32" Height="32" Background="Transparent">
    <Rectangle Fill="White" Height="20" Width="20" HorizontalAlignment="Center" VerticalAlignment="Center"/>
    <Rectangle Fill="White" Height="16" Width="4" HorizontalAlignment="Left" VerticalAlignment="Bottom"/>
    <Rectangle Fill="White" Width="16" Height="4" HorizontalAlignment="Left" VerticalAlignment="Bottom"/>
    <Rectangle Fill="White" Height="16" Width="4" HorizontalAlignment="Right" VerticalAlignment="Top"/>
    <Rectangle Fill="White" Width="16" Height="4" HorizontalAlignment="Right" VerticalAlignment="Top"/>
  </Grid>
</UserControl>